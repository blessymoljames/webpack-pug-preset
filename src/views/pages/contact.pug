  //script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js')

  .mymodel
    h2 Centered Modal Example
    p
      | Center the modal vertically and horizontally within the page, with the .modal-dialog-centered class.
    // Button to Open the Modal
    button.btn.btn-primary(type='button', data-toggle='modal', data-target='#myModal')
      | Open modal
    // The Modal
    #myModal.modal.fade
      .modal-dialog.modal-dialog-centered
        .modal-content
          // Modal Header
          .modal-header
            h4.modal-title Modal Heading
            button.close(type='button', data-dismiss='modal') &times;
          // Modal body
          .modal-body
            | Modal body..
          // Modal footer
          .modal-footer
            button.btn.btn-secondary(type='button', data-dismiss='modal') Close
